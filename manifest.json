{
    "manifest_version": 2,

    "name": "Air Hockey",
    "description": "This is a fun air hockey game.",
    "version": "1.0",
    "icons": {
        "128": "128airHockey.png",
        "48":"48airHockey.png",
        "16":"16airHockey.png"
    },

    "browser_action": {
    "default_icon": "16airHockey.png",
    "default_popup": "menu.html",
    "default_title": "Air Hockey"
    },
        "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}